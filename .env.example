# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_secret_key

# JWT Configuration
SECRET_KEY=your-long-random-secret-key-here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# SendGrid Configuration for Password Reset Emails
# Get API Key from: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=your-verified-sender@gmail.com

# OpenAI Configuration (optional, for chatbot)
OPENAI_API_KEY=your_openai_api_key

# Model Configuration
MODEL_NAME=brain_tumor_model.h5
IMAGE_SIZE=224
MAX_UPLOAD_SIZE=10485760
CONFIDENCE_THRESHOLD=0.5

# Logging
LOG_LEVEL=INFO
